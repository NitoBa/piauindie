    "@emotion/styled": "^11.10.5",
    "@graphql-codegen/typescript": "^2.8.3",
    "@graphql-codegen/typescript-operations": "^2.5.8",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "framer-motion": "^7.6.19",
    "graphql": "^16.6.0",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.2.0",
    "@graphql-codegen/cli": "^2.15.0",
    "@types/node": "18.11.11",
